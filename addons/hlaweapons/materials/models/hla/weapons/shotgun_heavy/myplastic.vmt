"UnLitGeneric"
{
	// Original shader: VertexLitMaskedEnvMappedTexture
	"$basetexture" "models/hla/weapons/shotgun_heavy/myplastic"
	"$additive" "1"
	"$translucent" "1"
//	"$nocull" "1"
	"$model" "1"
 	"$dudvmap" "models/hla/weapons/shotgun_heavy/strider_bulge_dudv"
 	"$normalmap" "models/hla/weapons/shotgun_heavy/strider_bulge_normal"
	"$bumpframe" "0"
	"$ignorez" "1"
	"$forcerefract" 1
	"$nofog" "1"


"Proxies"
{
 "AnimatedTexture"
 {
  "animatedtexturevar" "$basetexture"
  "animatedtextureframenumvar" "$frame"
  "animatedtextureframerate" 15 
 }
}

}


