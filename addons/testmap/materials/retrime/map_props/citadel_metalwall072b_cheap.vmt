// envmaptint_fix
"vertexlitgeneric"
{
	"$basetexture" "metal/metalwall072b"
	"$surfaceprop" "metal"
	"%keywords" "combine"

	// note: even though env_cubemap is here, it will turned off for dx6
	// because $multipass 1 isn't defined
	"$envmap" "env_cubemap"
	"$envmaptint" "[ .56 .56 .75 ]"
	"$envmapcontrast" 1
	"$envmapsaturation" 1

	"vertexlitgeneric_hdr_dx9"
	{
		"$bumpmap" "metal/metalwall072b_normal"
		"$normalmapalphaenvmapmask" 1
		"$envmaptint" "[ .09 .1 .12]"
	}
	"vertexlitgeneric_dx9"
	{
		"$bumpmap" "metal/metalwall072b_normal"
//		"$parallaxmap" "metal/metalwall072b_height"
//		"$parallaxmapscale" ".05"
		"$normalmapalphaenvmapmask" 1
		"$envmaptint" "[ .09 .1 .12]"
	}

	"vertexlitgeneric_dx8"
	{
		"$basetexture" "metal/citadel_metalwall072b"
		"$basealphaenvmapmask" 1
	}

	"vertexlitgeneric_nobump_dx8"
	{
		"$basetexture" "metal/citadel_metalwall072b"
		"$basealphaenvmapmask" 1
	}
}